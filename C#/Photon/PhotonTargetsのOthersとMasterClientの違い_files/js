        $('.js-category-wrapper .ui-tabs-nav').removeClass('ui-tabs-nav');

        // globalトップはキャッシュされてるので、jsでviaの引き継ぎをする
        var via = Hatena.Diary.Location.param('via');
        if (via) {
            Hatena.Diary.inheritVia(via);
        }
    };

})(jQuery);
