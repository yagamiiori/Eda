    $elements.each(function() {
        var $element = $(this);
        var username = $element.attr('data-user-name');
        if (friendships[username] && username != friendships[username].name) {
            var html = template({
                nickname  : friendships[username].name,
                hatenaId : username
            });

            $element.html(html);
        }
    });
